@using Web.Models
@model DetailsModel

<!DOCTYPE html>
<html>
<head>
    <title>www.nuritionalvalues.cf - nutritional data, nutritional values - @Model.Name</title>
    <meta charset="UTF-8">
    <meta content="@string.Format("nutritional data, nutritional values, {0}", Model.Name)" name="Description">
    <meta http-equiv="pragma" content="no-cache">
    <meta name="distribution" content="global">
    <meta name="classification" content="nutrition">
    <meta name="rating" content="general">
    <meta name="keywords" content="@Model.Name">
    <meta name="page-topic" content="@string.Format("www.nuritionalvalues.cf - nutritional data, nutritional values, {0}", Model.Name)>
    <meta name="Date" content="2016/03/26">
    <meta name="Robots" content="index,follow">
    <meta name="revisit-after" content="1 Days">
    <meta name="google-site-verification" content="7NfzK_5w2PAx45zyxjOVIMf0D7TnnTvHDLQoo4fDfoI" />

    <link rel="stylesheet" type="text/css" href="bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="basic-js-cookie-banner.min.css">
    <script src="jquery.min.js"></script>
    <script src="bootstrap.min.js"></script>
    <script src="basic-js-cookie-banner.js"></script>
</head>

<body>

    <div class="container">
        <div class="col-md-12">
            <header>
                <nav class="navbar navbar-default">
                    <div class="container">
                        <div class="navbar-header">
                            <a class="navbar-brand" href="#">Nutritional Values</a>
                        </div>
                        <ul class="nav navbar-nav navbar-left">
                            <li><a href="index.html"><i class="fa fa-shield"></i>List</a></li>
                        </ul>
                        <ul class="nav navbar-nav navbar-left">
                            <li><a href="about.html"><i class="fa fa-shield"></i>About</a></li>
                        </ul>
                    </div>
                </nav>
            </header>

            <div id="main">
                <div class="col-md-3">
                    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                    <!-- Left -->
                    <ins class="adsbygoogle" style="display: block" data-ad-client="ca-pub-1267228462498826" data-ad-slot="2971427998" data-ad-format="auto"></ins>
                    <script>
                        (adsbygoogle = window.adsbygoogle || []).push({});
                    </script>
                </div>
                <div class="col-md-6">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">Nutritional values</h3>
                        </div>
                        <div class="panel-body">
                            <dl class="dl-horizontal">
                                <dt>Name:</dt>
                                <dd>@Model.Name</dd>
                                <dt>Weight:</dt>
                                <dd>100 g</dd>
                                <dt>Kcal:</dt>
                                <dd>@Model.Calories</dd>
                                <dt>Carbohydrate:</dt>
                                @if (Model.Carbohydrate.HasValue)
                                {
                                    <dd>@Model.Carbohydrate.Value g</dd>
                                }
                                else
                                {
                                    <dd>-</dd>
                                }
                                <dt>Protein:</dt>
                                @if (Model.Protein.HasValue)
                                {
                                    <dd>@Model.Protein.Value g</dd>
                                }
                                else
                                {
                                    <dd>-</dd>
                                }
                                <dt>Fiber:</dt>
                                @if (Model.Fiber.HasValue)
                                {
                                    <dd>@Model.Fiber.Value g</dd>
                                }
                                else
                                {
                                    <dd>-</dd>
                                }
                                <dt>Sugar:</dt>
                                @if (Model.Sugar.HasValue)
                                {
                                    <dd>@Model.Sugar.Value g</dd>
                                }
                                else
                                {
                                    <dd>-</dd>
                                }
                                <dt>Fat:</dt>
                                @if (Model.Fat.HasValue)
                                {
                                    <dd>@Model.Fat.Value g</dd>
                                }
                                else
                                {
                                    <dd>-</dd>
                                }

                            </dl>
                        </div>
                    </div>
                    <a class="btn btn-default" href="index.html">Back to list</a>
                </div>
                <div class="col-md-3">
                    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                    <!-- Right -->
                    <ins class="adsbygoogle" style="display: block" data-ad-client="ca-pub-1267228462498826" data-ad-slot="7401627592" data-ad-format="auto"></ins>
                    <script>
                        (adsbygoogle = window.adsbygoogle || []).push({});
                    </script>
                </div>
            </div>
        </div>
    </div>
    <div class="alert alert-warning alert-dismissible cookie-banner" role="alert">
        <div style="display: none;" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></div>
        We use cookies to improve your experience. Please read our <a href="http://www.google.com/intl/en/policies/privacy/partners/">cookie policy</a>        for more information about how we use cookies.
    </div>
    <div class="container">
        <center>
            <!-- stat.4u.pl NiE KaSoWaC -->
